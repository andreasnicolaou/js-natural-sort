{
  "name": "@andreasnicolaou/js-natural-sort",
  "version": "1.2.0",
  "description": "Natural sorting of strings and numbers, supporting date, IP, and hex value comparison, with configurable case sensitivity and sorting order.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js",
    "umd": "./dist/index.umd.js",
    "umd-min": "./dist/index.umd.min.js"
  },
  "private": false,
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "dist/index.js",
    "dist/index.cjs",
    "dist/index.umd.js",
    "dist/index.umd.min.js",
    "dist/index.d.ts",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint --fix",
    "test": "jest",
    "test:ci": "cross-env JEST_JUNIT_CLASSNAME=\"{filepath}\" jest --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:bundle": "npx bundlewatch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andreasnicolaou/js-natural-sort.git"
  },
  "bugs": {
    "url": "https://github.com/andreasnicolaou/js-natural-sort/issues"
  },
  "author": {
    "name": "Andreas Nicolaou",
    "email": "anicolaou66@gmail.com"
  },
  "license": "MIT",
  "bundlewatch": {
    "files": [
      {
        "path": "dist/index.js",
        "maxSize": "4kB",
        "compression": "gzip"
      },
      {
        "path": "dist/index.cjs",
        "maxSize": "4kB",
        "compression": "gzip"
      },
      {
        "path": "dist/index.umd.js",
        "maxSize": "4kB",
        "compression": "gzip"
      },
      {
        "path": "dist/index.umd.min.js",
        "maxSize": "2kB",
        "compression": "gzip"
      },
      {
        "path": "dist/index.d.ts",
        "maxSize": "2kB",
        "compression": "none"
      },
      {
        "path": "dist/index.esm.js",
        "maxSize": "4kB",
        "compression": "gzip"
      },
      {
        "path": "dist/index.cjs.js",
        "maxSize": "4kB",
        "compression": "gzip"
      },
      {
        "path": "dist/index.amd.js",
        "maxSize": "4kB",
        "compression": "gzip"
      }
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.(ts)$": [
        "ts-jest"
      ]
    },
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup-plugin-dts": "^6.2.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "jest-junit": "^16.0.0",
    "prettier": "3.6.2",
    "rollup": "^4.51.0",
    "ts-jest": "^29.4.4",
    "cross-env": "^10.0.0",
    "bundlewatch": "^0.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "natural",
    "sort",
    "javascript",
    "array",
    "frontend",
    "sorter",
    "numeric",
    "unicode",
    "algorithm",
    "date",
    "datetime",
    "desc",
    "case-insensitive"
  ],
  "dependencies": {
    "tslib": "^2.8.1"
  }
}
