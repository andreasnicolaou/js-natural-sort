{
  "name": "js-natural-sort",
  "version": "1.0.0",
  "description": "Natural sorting of strings and numbers, supporting date, IP, and hex value comparison, with configurable case sensitivity and sorting order.",
  "main": "dist/index.js",
  "private": false,
  "types": "dist/types/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "format": "prettier --write .",
    "lint": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andreasnicolaou/js-natural-sort.git"
  },
  "bugs": {
    "url": "https://github.com/andreasnicolaou/js-natural-sort/issues"
  },
  "author": {
    "name": "Andreas Nicolaou",
    "email": "anicolaou66@gmail.com"
  },
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.(ts)$": [
        "ts-jest"
      ]
    },
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.11",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.4",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "3.5.3",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "keywords": [
    "natural",
    "sort",
    "javascript",
    "array",
    "frontend",
    "typescript"
  ]
}
